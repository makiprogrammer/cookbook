{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "unknown",
          "internal",
          ["parent", "sibling"],
          "index"
        ],
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
